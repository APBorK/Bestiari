    get { return state; }\r\n    }\r\n}\r\n\r\nclass Caretaker\r\n{\r\n    private
    List<Memento> mementos = new List<Memento>();\r\n\r\n    public void Add(Memento
    memento)\r\n    {\r\n        mementos.Add(memento);\r\n    }\r\n\r\n    public
